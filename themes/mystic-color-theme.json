{
	"name": "mystic-theme",
	"type": "dark",		
	"colors": {
		//Main colors editor
		"editor.background": "#0f0f0f",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#ae28fc",

		//Aside
		"sideBar.background": "#161616",
		"sideBarTitle.foreground": "#ffffff",

		"activityBar.activeBackground": "#ae28fc3b",
		"activityBar.activeBorder": "#ae28fc",
		"activityBar.background": "#0f0f0f",
		"activityBar.foreground": "#ffffff", //Color icons
		"activityBar.inactiveForeground": "#6e6e6e",

		//Badge
		"badge.background": "#ae28fc",
		"badge.foreground": "#ffffff",

		//Buttons
		"button.background": "#02973b",
		"button.border": "#ffffff00",

		//Inputs
		"input.background": "#0f0f0f",
		"input.foreground": "#ffffff",
		"focusBorder": "#ae28fc",

		//Top header
		"titleBar.activeBackground": "#0f0f0f",

		//Menu drop
		"menu.background": "#161616",
		"menu.selectionBackground": "#ae28fc",

		//Tabs
		"tab.lastPinnedBorder": "#ae28fc",
		"tab.activeBackground": "#0f0f0f",
		"tab.inactiveBackground": "#2b2b2b",
		"tab.unfocusedHoverBackground": "#0f0f0f",

		//Notification
		"notifications.background": "#0f0f0f",
		
		"textLink.foreground": "#b27ff7",
		"textLink.activeForeground": "#ffffff",

		//Footer
		"statusBar.background": "#ae28fc"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#686868"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"				
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#B27FF7"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				// "punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Tag",
			"scope": [				
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [				
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#33FF8B"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFB300"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other",								
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFB300"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant.language",				
			],
			"settings": {
				"foreground": "#ff4545"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"entity.name",
				"variable.other"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",				
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#84dbfd"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#b27ff7"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [				
				"entity.other.attribute-name",
				"support.type"
			],
			"settings": {
				"foreground": "#33FF8B"
			}
		},
		{
			"name": "CSS Attribuites",
			"scope": [				
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
